<body>
    <div class="box row">
        <div class="column" id="imaged">
            <img src="../image/addpub.jpg" style="height:450px;width:400px; margin:70px 70px 70px 70px;">
        </div>
    
    <div class="menu column">
    <form>
        <!-- <div class="input-container">
            <label for="doctype">Enter Teacher ID:</label>
            <input type="string" id="doctype" name="doctype" placeholder="Teacher ID"><br>
        </div> -->
        <div class="input-container">
